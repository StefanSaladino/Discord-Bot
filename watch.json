{
    "install": {
      "include": [
        "^package\\.json$",
        "^\\.env$"
      ]
    },
    "restart": {
      "exclude": [
        "^public/",
        "^dist/"
      ],
      "include": [
        "^\\.env"
      ]
    },
    "throttle": 900000
  }
